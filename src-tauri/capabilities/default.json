{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "fs:allow-read",
      "allow": [
        {
          "path": "$DOCUMENT/**"
        },
        {
          "path": "$HOME/**"
        },
        {
          "path": "$DESKTOP/**"
        },
        {
          "path": "$DOWNLOAD/**"
        },
        {
          "path": "$APPLOCALDATA/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-write",
      "allow": [
        {
          "path": "$DOCUMENT/**"
        },
        {
          "path": "$HOME/**"
        },
        {
          "path": "$APPLOCALDATA/**"
        }
      ]
    },
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "fs:allow-remove",
    "dialog:default"
  ]
}